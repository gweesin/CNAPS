{
  "name": "cnaps",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.9.0",
  "description": "CNAPS 大额支付号、联行号数据",
  "author": "gweesin<gweesin2000@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gweesin/CNAPS#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gweesin/CNAPS.git"
  },
  "bugs": "https://github.com/gweesin/CNAPS/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --no-warn-ignored",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "prestart": "esno scripts/check-charset.ts",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "async": "^3.2.6",
    "axios": "^1.7.5",
    "axios-retry": "^4.5.0",
    "diff": "^6.0.0-beta",
    "execa": "^9.3.1",
    "fs-extra": "^11.2.0",
    "pino": "^9.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@antfu/ni": "^0.22.4",
    "@antfu/utils": "^0.7.10",
    "@types/async": "^3.2.24",
    "@types/diff": "^5.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.5.1",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "lint-staged": "^15.2.9",
    "pnpm": "^9.9.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "unbuild": "^2.0.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
